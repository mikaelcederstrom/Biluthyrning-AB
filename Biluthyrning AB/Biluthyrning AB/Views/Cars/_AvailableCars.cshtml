@model Biluthyrning_AB.Models.ViewModels.CarsListOfAllVM[]
<select name="CarId">

    @for (int i = 0; i < Model.Length; i++)
    {
        @if (Model[i].AvailableForRent == true)
        {
            <option value="@Model[i].Id">@Model[i].CarType, @Model[i].Registrationnumber</option>
        }
    }
</select>
<br />
<input type="submit" value="Boka" />
